<table class="user-list">
    <th>
        Username
    </th>
    <th>
        First Name
    </th>
    <th>
        Last Name
    </th>
    <% if(userData.length === 0){ %>
    <tr>
        <td colspan="3">
            <% if(extras.isTableMembers){ %>
                This table has no members.
            <% } else { %>
                No Users Found
            <% } %>
        </td>
    </tr>
    <% } else { %>
    <% userData.forEach(function(user){ %>
    <tr>
        <td>
            <a href="/users/id/<%=user._id%>"><%=user.username%></a>
        </td>
        <td>
            <%=user.firstName%>
        </td>
        <td>
            <%=user.lastName%>
        </td>
    </tr>
    <% }); %>
    <% } %>
</table>